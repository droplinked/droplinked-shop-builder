{
	"compilerOptions": {
		"resolveJsonModule": true,
		"baseUrl": "src",
		"jsx": "react",
		"module": "esnext",
		"moduleResolution": "bundler",
		"target": "esnext",
		"lib": ["esnext", "dom", "dom.iterable"],
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": false
	},
	"include": ["src"]
}
